{
    "name": "Cookie Clearer",
    "description": "Simple cookie cleaner.",
    "manifest_version": 3,
    "version": "1.0.0",
    "default_locale": "de_DE",
    "permissions": ["cookies"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    }
  }